##### Properties ---------#########
####-#############################


#width

wa:width a rdf:Property ;
        rdfs:label "width" ;
        rdfs:comment "" ;
        rdfs:domain wa:Step;
        rdfs:range wa:Measurement .

#is it width for the "trede"@nl?


#stairStep

wa:stairStep a rdf:Property ;
        rdfs:label "has stair step" ;
        rdfs:comment "" ;
        rdfs:domain wa:Stair;
        rdfs:range wa:Step .


#score

wa:score a rdf:Property ;
        rdfs:label "has score" ;
        rdfs:comment "" ;
        rdfs:domain wa:MeasurementScore;
        rdfs:range xsd:string .


#value

wa:value a rdf:Property ;
        rdfs:label "has width" ;
        rdfs:comment "" ;
        rdfs:domain wa:MeasurementScore;
        rdfs:range xsd:decimal .