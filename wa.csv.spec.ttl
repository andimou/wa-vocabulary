; :type schema:ImageObject
; :identity [
    :source_column "URL_FOTO_2";
    :base_uri ""
    ]
.

<#URL_TOELVA> a :Resource
; :type schema:ImageObject
; :identity [
    :source_column "URL_TOELVA";
    :base_uri ""
    ]
.

<#URL_BROCHURE> a :Resource
